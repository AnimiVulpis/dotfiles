[
    {
        "command": "-liveshare.join",
        "key": "ctrl+alt+j",
        "when": "liveshare:state != 'Joined' && liveshare:state != 'Shared'"
    },
    {
        "command": "liveshare.join",
        "key": "ctrl+alt+j",
        "when": "explorerViewletFocus && liveshare:state != 'Joined' && liveshare:state != 'Shared'"
    }
]
