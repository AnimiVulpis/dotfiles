            "hovers": true,
            "name": "Review",
            "statusBar": false,
            "statusBarItemName": "Review"
        },
        "zen": {
            "codeLens": false,
            "currentLine": false,
            "description": "for a zen-like experience, disables many visual features",
            "hovers": false,
            "name": "Zen",
            "statusBar": false,
            "statusBarItemName": "Zen"
        }
    },
    "gitlens.statusBar.enabled": false,
