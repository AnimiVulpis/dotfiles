{
    "Lua.telemetry.enable": false,
    "[lua]": {
        "editor.defaultFormatter": "Koihik.vscode-lua-format"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "auto-close-tag.SublimeText3Mode": true,
    "auto-close-tag.insertSpaceBeforeSelfClosingTag": true,
    "bookmarks.useWorkaroundForFormatters": true,
    "cSpell.allowCompoundWords": true,
    "cSpell.language": "en,de",
    "cSpell.userWords": [
        "animi",
        "fwupd",
        "fwupdmgr",
        "saas"
    ],
    "color-highlight.markRuler": false,
    "color-highlight.markerType": "outline",
    "color-highlight.matchWords": true,
    "csv-preview.lineNumbers": "ordinal",
    "debug.console.fontFamily": "Iosevka Fixed",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "solid",
    "editor.cursorSurroundingLines": 5,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.dragAndDrop": false,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontFamily": "'Iosevka', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.guides.bracketPairs": true,
    "editor.linkedEditing": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.renderWhitespace": "boundary",
    "editor.roundedSelection": false,
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.semanticHighlighting.enabled": true,
    "editor.showFoldingControls": "always",
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.showStatusBar": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.wordBasedSuggestionsMode": "allDocuments",
    "explorer.autoReveal": false,
    "explorer.incrementalNaming": "smart",
    "extensions.autoUpdate": false,
    "files.associations": {
        "config": "jsonc"
    },
    "files.enableTrash": false,
    "git.mergeEditor": true,
    "gitlens.blame.avatars": false,
    "gitlens.blame.highlight.locations": [
        "gutter",
        "line",
        "overview"
    ],
    "gitlens.changes.locations": [
        "gutter",
        "overview"
    ],
    "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm ZZ",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "gitlens.defaultGravatarsStyle": "retro",
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.heatmap.locations": [
        "gutter"
    ],
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.mode.active": "zen",
    "gitlens.statusBar.enabled": false,
    "html.format.endWithNewline": true,
    "liveshare.increasedGuestLimit": false,
    "liveshare.sharedTerminalHeight": 100,
    "path-intellisense.showHiddenFiles": true,
    "problems.showCurrentInStatus": true,
    "redhat.telemetry.enabled": false,
    "scm.inputFontFamily": "editor",
    "screencastMode.onlyKeyboardShortcuts": true,
    "search.quickOpen.history.filterSortOrder": "recency",
    "search.searchEditor.reusePriorSearchConfiguration": true,
    "search.searchOnTypeDebouncePeriod": 500,
    "shellformat.flag": "-i 4 -bn -ci -sr",
    "shellformat.path": "/home/user/go/bin/shfmt",
    "telemetry.telemetryLevel": "off",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.fontFamily": "Iosevka Fixed",
    "terminal.integrated.scrollback": 4200,
    "terminal.integrated.shellIntegration.enabled": true,
    "todo-tree.filtering.includeHiddenFiles": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.autoDetectHighContrast": false,
    "window.menuBarVisibility": "toggle",
    "window.title": "VSCode${separator}${rootName}${separator}${activeEditorMedium}${dirty}",
    "workbench.colorTheme": "Solarized Dark",
    "workbench.commandPalette.preserveInput": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.editor.sharedViewState": true,
    "workbench.editor.tabCloseButton": "off",
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.preferredDarkColorTheme": "Solarized Dark",
    "workbench.preferredLightColorTheme": "Solarized Light",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "workbench.tree.indent": 6,
    "workbench.tree.renderIndentGuides": "always",
    "zenMode.fullScreen": false,
    "zenMode.restore": false,
    "zenMode.silentNotifications": false
}
