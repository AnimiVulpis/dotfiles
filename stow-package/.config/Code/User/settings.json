{
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "alexLinter.check.enabled": true,
    "auto-close-tag.SublimeText3Mode": true,
    "auto-close-tag.enableAutoCloseTag": false,
    "bookmarks.showCommandsInContextMenu": false,
    "bookmarks.useWorkaroundForFormatters": true,
    "cSpell.allowCompoundWords": true,
    "cSpell.language": "en,de",
    "cSpell.userWords": [
        "animi",
        "esbenp",
        "fluentui",
        "luma",
        "msrvida",
        "work",
        "theia"
    ],
    "color-highlight.markRuler": false,
    "color-highlight.markerType": "dot-after",
    "debug.inlineValues": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "solid",
    "editor.cursorSurroundingLines": 2,
    "editor.fontFamily": "'iosevka', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.renderWhitespace": "boundary",
    "editor.roundedSelection": false,
    "editor.showFoldingControls": "always",
    "editor.suggest.localityBonus": true,
    "editor.suggest.statusBar.visible": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "eslint.codeActionsOnSave.mode": "problems",
    "explorer.autoReveal": "focusNoScroll",
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.enableTrash": false,
    "files.hotExit": "off",
    "gitlens.advanced.telemetry.enabled": false,
    "gitlens.autolinks": [
        {
            "ignoreCase": true,
            "prefix": "CJM-",
            "url": "https://jira.work.com/browse/CJM-<num>"
        },
        {
            "ignoreCase": true,
            "prefix": "DES-",
            "url": "https://jira.work.com/browse/DES-<num>"
        },
        {
            "ignoreCase": true,
            "prefix": "SPM-",
            "url": "https://jira.work.com/browse/SPM-<num>"
        }
    ],
    "gitlens.blame.dateFormat": "YYYY-MM-DDTHH:mmZ",
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.defaultDateFormat": "YYYY-MM-DDTHH:mmZ",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "gitlens.hovers.annotations.over": "annotation",
    "gitlens.liveshare.allowGuestAccess": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "html.format.endWithNewline": true,
    "json.schemaDownload.enable": false,
    "liveshare.allowGuestDebugControl": true,
    "liveshare.allowGuestTaskControl": true,
    "liveshare.increasedGuestLimit": false,
    "liveshare.languages.allowGuestCommandControl": true,
    "liveshare.presence": true,
    "liveshare.publishWorkspaceInfo": true,
    "liveshare.showInStatusBar": "whileCollaborating",
    "npm.fetchOnlinePackageInfo": false,
    "prettier.packageManager": "yarn",
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "problems.showCurrentInStatus": true,
    "screencastMode.onlyKeyboardShortcuts": true,
    "search.searchEditor.reusePriorSearchConfiguration": true,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.confirmOnExit": true,
    "terminal.integrated.scrollback": 4200,
    "typescript.disableAutomaticTypeAcquisition": true,
    "update.mode": "start",
    "vulnCost.sendStatistics": false,
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "folders",
    "window.title": "${rootName}${separator}${activeEditorShort}${dirty}",
    "workbench.colorTheme": "Solarized Dark",
    "workbench.commandPalette.preserveInput": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabCloseButton": "off",
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.preferredDarkColorTheme": "Solarized Dark",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "workbench.tree.renderIndentGuides": "always"
}
